#include <stdio.h>
void main() 
{
    int a,i,count;
    printf("Enter a number:");
    scanf("%d",&a);
    if(a==2)
        printf("It is prime");
    else
    {
       for(i=2;i<a;i++) 
       {
           if(a%i==0)
           {
              count++;
           }
       }
           if(count==0)
               printf("It is prime");
            else
               printf("It is not prime");
    }
}
