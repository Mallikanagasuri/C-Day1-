#include<stdio.h>
void prime(int n);
int main()
{
  int a;
  printf("Enter any number:");
  scanf("%d",&a);
  printf("The prime numbers before %d are:\n",a);
  prime(a);
  return 0;
}
void prime(int n)
{
 int c=0;
 if(n!=1)
 {
 for(int i=2;i<n;i++)
 {
  if(n%i==0)
  {
    c=1;
  }  
 }   
 if(c==0)
 {
    printf("%d\n",n);
 }  
 prime(n-1); 
 }
}  
